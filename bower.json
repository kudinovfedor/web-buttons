{
  "name": "fk-template",
  "version": "1.0.0",
  "description": "Workpieces code and structure that I use in every project",
  "main": "gulpfile.js",
  "authors": [
    "Kudinov Fedor"
  ],
  "license": "MIT",
  "keywords": [
    "HTML5",
    "CSS3",
    "JS",
    "jQuery",
    "Pug(Jade)",
    "Compass+Sass",
    "Sprites",
    "Favicon",
    "SVG",
    "SMACSS",
    "Ruby",
    "gem",
    "Node",
    "npm",
    "Gulp",
    "Modernizr",
    "Bower",
    "Browsersync",
    "ESLint",
    "JSHint",
    "HTMLHint",
    "Autoprefixer",
    "Babel",
    "Webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kudinovfedor/fk-template.git"
  },
  "homepage": "https://github.com/kudinovfedor/fk-template",
  "moduleType": [
    "amd",
    "es6",
    "globals",
    "node",
    "yui"
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "html5shiv": "~3.7.3",
    "jquery": "~1.12.4 < 2",
    "modernizr": "~3.3.1",
    "pixlayout": "~0.9.7",
    "respond": "~1.4.2",
    "compass-mixins": "^1.0.2"
  },
  "overrides": {
    "html5shiv": {
      "main": [
        "./dist/html5shiv.min.js"
      ]
    },
    "jquery": {
      "main": [
        "./dist/jquery.min.js"
      ]
    },
    "modernizr": {
      "ignore": true
    },
    "pixlayput": {
      "main": [
        "./jquery.pixlayout.min.js"
      ]
    },
    "respond": {
      "main": [
        "./dest/respond.min.js"
      ]
    },
    "compass-mixins": {
      "ignore": true
    }
  }
}
